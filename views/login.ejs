<%- include('header'); -%>


<form action="/login"  method="Post" class=" col-lg-3 col-md-4 m-auto mt-5 p-4">
  <div class="mb-3 mt-5 ">
    <label for="exampleInputEmail1" class="form-label  bg-secondary p-2 rounded-3 text-white">Email address</label>
    <input type="email" class="form-control" name="email" id="exampleInputEmail1" aria-describedby="emailHelp" required autocomplete="off">
  </div>
  <div class="mb-3">
    <label for="exampleInputPassword1" class="form-label bg-secondary p-2 rounded-3 text-white" autocomplete="off">Password</label>
    <input type="password" class="form-control" name="password" id="exampleInputPassword1" required>
    <div id="passwordHelp" class="form-text">Never share your Credentials with anyone else.</div>
  </div>
  <div class="mb-1 float-end">
    <label class="form-text fs-5">&nbsp&nbsp New Here.  <a href="/Signup" class="text-decoration-none text-primary">Signup</a></label>
  </div>
  <button type="submit" class="btn btn-outline-secondary" name="submit">Login</button>
</form>

<%- include('footer'); -%>